{
  "name": "ng-monorepo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start-one",
    "build-one": "ng build --project=app-number-one --localize",
    "watch-one": "ng build --project=app-number-one --localize --watch --configuration development",
    "test-one": "ng test --project=app-number-one",
    "start-one": "ng serve --project=app-number-one --configuration=en-US",
    "start-one:pt-BR": "ng serve --project=app-number-one --configuration=pt-BR",
    "start-one:ko-KR": "ng serve --project=app-number-one --configuration=ko-KR",
    "build-two": "ng build --project=app-number-two --localize",
    "watch-two": "ng build --project=app-number-two --localize --watch --configuration development",
    "test-two": "ng test --project=app-number-two",
    "start-two": "ng serve --project=app-number-two --configuration=en-US",
    "start-two:pt-BR": "ng serve --project=app-number-two --configuration=pt-BR",
    "start-two:ko-KR": "ng serve --project=app-number-two --configuration=ko-KR",
    "extract-locales-one": "ng extract-i18n --project=app-number-one --format=json --output-path src/locale/app-number-one",
    "extract-locales-two": "ng extract-i18n --project=app-number-two --format=json --output-path src/locale/app-number-two"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/localize": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/localize": "^14.2.12",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.2"
  }
}